{
  "schemaVersion": 1,
  "id": "touchstonedst",
  "version": "${version}",

  "name": "TouchStone DST",
  "description": "This is a mod that adds in Touch Stones from Don't Starve Together at the cost of no more Totem of Undying",
  "authors": [
    "The Changer"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/touchstonedst/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.the_changer.touchstonedst.TouchStoneDST"
    ]
  },
  "mixins": [
    "touchstonedst.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
