{
  "schemaVersion": 1,
  "id": "touchstonedst",
  "version": "${version}",

  "name": "DST TouchStone In Minecraft",
  "description": "This Mod will add in the DST touch stone into Minecraft at the cost of the Totem of Undying being removed from the game. Note that the mod was designed for hardcore mode.",
  "authors": [
    "The Changer"
  ],
  "contact": {
    "homepage": "https://github.com/The-Changer412/DST-Touch-Stone-In-Minecraft",
    "sources": "https://github.com/The-Changer412/DST-Touch-Stone-In-Minecraft"
  },

  "license": "GPL-3.0 License",
  "icon": "assets/touchstonedst/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.the_changer.touchstonedst.TouchStoneDST"
    ],
    "client": [
      "com.the_changer.touchstonedst.TouchStoneDSTClientInit"
    ]
  },
  "mixins": [
    "touchstonedst.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
